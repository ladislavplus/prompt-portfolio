{
    "name": "translation_en_to_fr",
    "description": "A set of test cases for evaluating English to French translation prompts.",
    "test_cases": [
        {
            "id": "en_fr_001",
            "input": {
                "source_language": "English",
                "target_language": "French",
                "text_to_translate": "Hello, how are you?"
            },
            "expected_output": "Bonjour, comment ça va ?"
        },
        {
            "id": "en_fr_002",
            "input": {
                "source_language": "English",
                "target_language": "French",
                "text_to_translate": "I would like to order a coffee."
            },
            "expected_output": "Je voudrais commander un café."
        },
        {
            "id": "en_fr_003",
            "input": {
                "source_language": "English",
                "target_language": "French",
                "text_to_translate": "The quick brown fox jumps over the lazy dog."
            },
            "expected_output": "Le rapide renard brun saute par-dessus le chien paresseux."
        }
    ]
}